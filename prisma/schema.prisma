generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model test {
  id   Int    @id
  name String @db.VarChar(50)
}

model products {
  id          Int     @id @default(autoincrement())
  name        String  @db.VarChar(100)
  price       Decimal @db.Decimal
  description String  @db.VarChar(500)
  stock       Int
  images      Int
}
